generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}

model UserInfo {
    id           String @id @default(auto()) @map("_id") @db.ObjectId
    Name         String
    Email        String
    Profile      String
    ProjectCount String
    StorageSize  String
}

model Projects {
    id          String @id @default(auto()) @map("_id") @db.ObjectId
    projectname String
    projectsize String
    template    String
    userid      String
    created_on  String
}

model Template_Userinfo {
    id         String   @id @default(auto()) @map("_id") @db.ObjectId
    Name       String
    Surname    String
    Email      String
    Age        String
    Created_on DateTime @default(now())
    projectid  String
}

model Template_Feedback {
    id         String   @id @default(auto()) @map("_id") @db.ObjectId
    Name       String
    Email      String
    Feedback   String
    Created_on DateTime @default(now())
    projectid  String
}

model Template_NewsLetter {
    id         String   @id @default(auto()) @map("_id") @db.ObjectId
    Name       String
    Email      String
    Created_on DateTime @default(now())
    projectid  String
}

model Template_ContactUs {
    id          String   @id @default(auto()) @map("_id") @db.ObjectId
    Name        String
    Email       String
    Country     String
    PhoneNumber String
    Message     String
    Created_on  DateTime @default(now())
    projectid   String
}

model Template_ContactUs2 {
    id         String   @id @default(auto()) @map("_id") @db.ObjectId
    Name       String
    Email      String
    Country    String
    Message    String
    Created_on DateTime @default(now())
    projectid  String
}
