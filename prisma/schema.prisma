generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}

model UserInfo {
    id           String @id @default(auto()) @map("_id") @db.ObjectId
    Name         String
    Email        String
    Profile      String
    ProjectCount String
    StorageSize  String
}
